{
  "name": "create-npm",
  "version": "0.0.21",
  "description": "Bootstrap npm packages using Yarn",
  "keywords": [
    "yarn",
    "npm",
    "bootstrap"
  ],
  "homepage": "https://github.com/vinsonchuong/create-npm",
  "bugs": "https://github.com/vinsonchuong/create-npm/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "main": "./src/index.js",
  "bin": "./src/bin/create-npm.js",
  "repository": "vinsonchuong/create-npm",
  "scripts": {
    "prepare": "flow-typed update --skip",
    "test": "standard -v --fix && flow && ava -v",
    "release": "semantic-release",
    "prepack": "build-esm"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@babel/register": "^7.0.0-rc.1",
    "ava": "^1.0.0-beta.7",
    "babel-preset-diff": "^6.0.1",
    "build-esm": "^4.0.0",
    "flow-bin": "^0.78.0",
    "flow-typed": "^2.5.1",
    "standard-esnext": "^5.1.3"
  },
  "babel": {
    "presets": [
      "diff"
    ]
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  }
}
